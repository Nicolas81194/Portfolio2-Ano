create table livros(
	quantEstoque varchar(100) not null,
    genero varchar(7) not null,
    autor varchar (14) not null,
  	titulo varchar(14) not null,
    primary key (titulo)
  )
 
   Create table Fornecedores(
    nome varchar(7) not null,
    telefone varchar(11) not null,
    codFornecedor varchar(14) not null,
    primary key (codFornecedor)
    )
    
